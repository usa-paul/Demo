{
  "name": "security-enterprise-admin",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run build:prod",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "build:sit": "vue-tsc --noEmit && vite build --mode production.sit",
    "build:uat": "vue-tsc --noEmit && vite build --mode production.uat",
    "serve": "npm run build && vite preview"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "git add"
    ]
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "4.0.3",
    "@vueuse/core": "^5.0.3",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.14.0",
    "element-plus": "^2.1.0",
    "lodash": "^4.17.21",
    "mitt": "^2.1.0",
    "monaco-editor": "^0.25.2",
    "nprogress": "^0.2.0",
    "qs": "^6.10.1",
    "quill": "^1.3.7",
    "sortablejs": "^1.13.0",
    "vue": "^3.2.0",
    "vue-i18n": "^10.0.0-beta.1",
    "vue-router": "4.0.10",
    "vuex": "4.0.2",
    "vxe-table": "4.0.23",
    "xe-utils": "3.3.0"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.6",
    "@types/quill": "^2.0.8",
    "@types/sortablejs": "^1.10.6",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.4",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "~4.3.3",
    "vite": "^2.3.7",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-pwa": "^0.8.1",
    "vite-plugin-svg-icons": "^0.7.1",
    "vite-tsconfig-paths": "^3.3.13",
    "vue-tsc": "^0.1.7",
    "yorkie": "^2.0.0"
  }
}
